#!/usr/bin/env groovy
def call(branchName,urllink,dockerimage,mavenBuild,sonarorganization,sonarprojectKey,sonarprojectName){
        pipeline {
            agent any
            tools {
                   maven 'Maven_Home'
            }
            stages {
               
                stage('CleanUp'){
                    steps{
                         deleteDir()
                    }
                }
                stage('Code Checkout'){
                    steps{
                           checkOutFrom((branchName,urllink)
                    }
                }
               
                stage('Build_In_Container'){
                    steps{
                            dockerScript(dockerimage,mavenBuild)
                    }
                }
                stage('SonarStage'){
                    steps{
                       sonar_analyse(sonarorganization,sonarProjectkey,sonarProjectname)
                    }
                }
                stage('QualityGate'){
                    steps{
                       sonar_quality_gate()
                    }
                }
            }
      }
}
